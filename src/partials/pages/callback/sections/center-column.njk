      <div id="center-column" class="column col-center">
        <div class="info-card">
          <div
            class="history-filter-row">
            <h3 class="callback-notice-title">공지사항</h3><button
              class="callback-notice-more-btn">더보기 +</button>
          </div>
          <ul class="callback-notice-list">
            {% for notice in callback_data.notices or [] %}
              <li class="callback-notice-item"><span
                  class="callback-notice-text">{% if notice.prefix_text %}<span
                    class="{{ notice.prefix_class }}">{{ notice.prefix_text }}</span>{% endif %}{{ notice.text }}
                  {% if notice.has_attachment %}
                    <i
                      class="fa-solid fa-paperclip callback-icon-attachment"></i>
                  {% endif %}</span><span
                  class="callback-notice-date">{{ notice.date }}</span>
              </li>
            {% endfor %}
          </ul>
        </div>
        <div class="info-card">
          <div class="callback-section-header">
            <h3 class="callback-section-title">
              <i class="fa-solid fa-phone-volume callback-icon-phone-volume"></i>
              콜백 통계
            </h3><select
              class="callback-period-select">
              <option>오늘</option>
              <option>이번 주</option>
              <option>이번 달</option>
            </select>
          </div>
          <div class="callback-stats-grid">
            <div class="callback-stat-card callback-stat-card--pending">
              <div class="callback-stat-value-pending">3</div>
              <div class="callback-stat-label">대기중</div>
            </div>
            <div class="callback-stat-card callback-stat-card--progress">
              <div class="callback-stat-value-progress">8</div>
              <div class="callback-stat-label">처리중</div>
            </div>
            <div class="callback-stat-card callback-stat-card--completed">
              <div class="callback-stat-value-completed">12</div>
              <div class="callback-stat-label">완료</div>
            </div>
          </div>
          <div class="callback-stats-summary">
            <div class="callback-summary-row"><span
                class="callback-summary-label">평균 응답 시간</span><span
                class="callback-summary-value callback-summary-value--brand">8분 32초</span></div>
            <div class="callback-summary-rate-row"><span
                class="callback-summary-label">완료율</span><span
                class="callback-summary-value callback-summary-value--success">94.2%</span></div>
          </div>
        </div>
        <div class="info-card">
          <div class="callback-section-header">
            <h3 class="callback-section-title">
              <i class="fa-solid fa-clock-rotate-left callback-icon-section-title"></i>
              최근 콜백 처리 내역
            </h3><button
              class="callback-link-more">전체보기</button>
          </div>
          <div class="callback-request-list">
            {% for history in callback_data.recent_callback_histories or [] %}
              <div class="callback-history-card">
                <div class="callback-card-header-row"><span
                    class="callback-card-title">{{ history.name }}</span><span
                    class="callback-status-success-badge">{{ history.status }}</span>
                </div>
                <p class="callback-history-desc">
                  {{ history.desc }}
                </p>
                <div class="callback-history-meta-row"><span><i
                      class="fa-solid fa-phone callback-icon-inline"></i>{{ history.phone }}</span><span>{{ history.date }}</span></div>
              </div>
            {% endfor %}
          </div>
        </div>
      </div>
